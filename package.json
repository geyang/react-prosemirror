{
  "name": "react-es6-template",
  "version": "0.0.0",
  "description": "a template for react es6 components. Contains webpack configuration and document generator.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "babel-node src/index.js",
    "lint": "eslint src/**.js",
    "build": "babel -d dist/ src/",
    "prepublish": "npm run build",
    "tape": "babel-node node_modules/argg src/**.spec.js",
    "istanbul": "npm run build && istanbul cover --dir coverage/istanbul node_modules/argg dist/**.spec.js --report lcovonly",
    "coverage": "npm run build && istanbul cover --dir coverage/istanbul node_modules/argg dist/**.spec.js --report html",
    "coveralls": "cat ./coverage/istanbul/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "plato": "plato -d coverage/plato dist/index.js",
    "test": "npm run istanbul && npm run coveralls",
    "gh-pages": "git add dist && git ci -m 'BUILD DOC' && git subtree push --prefix dist/example origin gh-pages",
    "build-docs": "webpack --progress --profile --colors --config webpack.build-docs.config.js",
    "serve-docs": "webpack-dev-server --progress --profile --colors --hot --config webpack.develop.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/episodeyang/react-es6-template.git"
  },
  "keywords": [
    "react",
    "component",
    "es6",
    "component",
    "react-docgen",
    "docgen",
    "webpack",
    "react-hot-reload"
  ],
  "author": "Ge Yang <yangge1987@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/episodeyang/react-es6-template/issues"
  },
  "homepage": "https://github.com/episodeyang/react-es6-template#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "6.x",
    "babel-core": "^6.7.7",
    "babel-eslint": "6.x",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-2": "6.x",
    "babelify": "^7.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint": "2.x",
    "eslint-plugin-react": "5.x",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.4",
    "markdown-it": "^7.0.0",
    "markdown-it-loader": "^0.2.0",
    "markdown-to-jsx": "^2.0.1",
    "node-sass": "^3.7.0",
    "opn-cli": "1.x",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react": "15.x",
    "react-addons-test-utils": "^15.1.0",
    "react-component-props-table": "0.0.0",
    "react-docgen": "^2.8.2",
    "react-docgen-loader": "^2.9.1",
    "react-dom": "15.x",
    "react-highlight": "^0.8.0",
    "react-hot-loader": "^1.3.0",
    "react-markdownit": "^2.0.3",
    "run-browser-babel": "5.x",
    "sass-loader": "^3.2.0",
    "scss-loader": "0.0.1",
    "style-loader": "^0.13.1",
    "tape": "4.x",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
